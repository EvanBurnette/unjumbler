	--color-error-900: 66 66 66; /* ⬅ #424242 */
	/* surface | #aeab98 */
	--color-surface-50: 243 242 240; /* ⬅ #f3f2f0 */
	--color-surface-100: 239 238 234; /* ⬅ #efeeea */
	--color-surface-200: 235 234 229; /* ⬅ #ebeae5 */
	--color-surface-300: 223 221 214; /* ⬅ #dfddd6 */
	--color-surface-400: 198 196 183; /* ⬅ #c6c4b7 */
	--color-surface-500: 174 171 152; /* ⬅ #aeab98 */
	--color-surface-600: 157 154 137; /* ⬅ #9d9a89 */
	--color-surface-700: 131 128 114; /* ⬅ #838072 */
	--color-surface-800: 104 103 91; /* ⬅ #68675b */
	--color-surface-900: 85 84 74; /* ⬅ #55544a */
}
